HashKnownHosts yes

Host *
  AddKeysToAgent yes
{{- if eq .chezmoi.os "darwin" }}
  UseKeychain yes
{{- end }}
  PreferredAuthentications publickey

Host fslef.github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_gh_fslef

Host *.visualstudio.com
  IdentitiesOnly yes
  HostkeyAlgorithms +ssh-rsa
  PubkeyAcceptedAlgorithms +ssh-rsa

Host avnext.visualstudio.com
  HostName HostName ssh.dev.azure.com
  IdentityFile ~/.ssh/id_ado_fslef